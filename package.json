{
  "name": "cp-tools-electron-app",
  "version": "1.0.0",
  "description": "app for cp-tools, cp-tools for app",
  "main": "main.js",
  "scripts": {
    "setup-once": "electron-rebuild -f -w realm",
    "webpack:run": "webpack -p",
    "webpack:watch": "NDOE_ENV='development' webpack --watch",
    "start": "npm run webpack:run && NDOE_ENV='production' electron .",
    "cptools:bbl": "rm -rf local_modules/cp-tools/libcptools && babel local_modules/cp-tools/cptools -d local_modules/cp-tools/libcptools",
    "cptools:webpack": "webpack -p --config webpack.libcptools.config.js",
    "cptools:webpackwatch": "NDOE_ENV='development' webpack --watch --config webpack.libcptools.config.js",
    "cptools": "npm run cptools:bbl && npm run cptools:webpack",
    "debug": "NDOE_ENV='development' npm run cptools && npm run webpack:run && NODE_ENV='development' electron .",
    "debug:watchall": "NODE_ENV='development' electron .",
    "dist": "NODE_ENV='production' build -mw --x64",
    "sign": "codesign --deep --force --verbose --sign 'Mac Developer: XXXXXXXX (YYYYYYYY)' ./dist/mac/MyElectronApp.app",
    "release": "npm run dist && npm run sign"
  },
  "repository": "https://github.com/code-propagator/cp-tools-electron-app.git",
  "private": true,
  "keywords": [
    "Electron",
    "Realm",
    "React"
  ],
  "author": "code-propagator",
  "license": "MIT",
  "build": {
    "appId": "xxx.yyy.zzz"
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.7",
    "electron": "^1.8.1",
    "electron-builder": "^19.34.1",
    "electron-rebuild": "^1.6.0",
    "style-loader": "^0.18.2",
    "uglify-es-webpack-plugin": "^0.10.0"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "classnames": "^2.2.5",
    "cp-tools": "file:local_modules/cp-tools",
    "d3": "^4.11.0",
    "debug": "^3.1.0",
    "events": "^1.1.1",
    "faker": "^4.1.0",
    "fs": "0.0.1-security",
    "history": "^4.7.2",
    "html": "^1.0.0",
    "indent2obj": "0.0.3",
    "json-beautify": "^1.0.1",
    "json-easy-filter": "^0.3.1",
    "lodash": "^4.17.4",
    "marksy": "^5.0.0",
    "material-ui": "^0.19.3",
    "material-ui-bottom-sheet": "^0.4.1",
    "moment": "^2.18.1",
    "namor": "^1.0.1",
    "obj2indent": "0.0.1",
    "react": "^15.6.2",
    "react-aztec": "^0.1.36",
    "react-click-outside": "^3.0.0",
    "react-custom-scrollbars": "^4.1.2",
    "react-dom": "^15.6.2",
    "react-flexbox-grid": "^1.1.5",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-table": "^6.6.0",
    "react-tap-event-plugin": "^2.0.1",
    "react-tooltip": "^3.3.1",
    "react-variable-height-infinite-scroller": "^3.3.5",
    "realm": "^1.13.0",
    "redux": "^3.7.2",
    "redux-form": "^7.0.4",
    "request-promise": "^4.2.2",
    "sorty": "^1.2.2",
    "text-encoding": "^0.6.4",
    "uuid": "^3.1.0",
    "webpack": "^3.6.0"
  }
}
